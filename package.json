{
  "name": "sketchboard",
  "description": "TODO",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "homepage": "https://jackdbd.github.io/sketchboard",
  "repository": "git@github.com:jackdbd/sketchboard.git",
  "version": "0.1.0",
  "main": "./src/index.tsx",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn clean && node scripts/build.js",
    "ci": "yarn format:check && yarn test:unit && yarn coverage",
    "clean": "rimraf build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dev": "node scripts/start.js",
    "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check '{,!(node_modules)/}**/*.{js,jsx,ts,tsx,json}'",
    "lint:css": "stylelint --config .stylelintrc.json --formatter verbose './src/css/*.css' 'src/**/*.module.css'",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "predeploy": "yarn build",
    "stats": "webpack-bundle-analyzer build/stats.json --port 8888",
    "test": "yarn lint:css && concurrently \"yarn test:unit\" \"yarn test:regressions\"",
    "test:unit": "jest --testPathIgnorePatterns regressions --verbose",
    "test:regressions": "echo 'TODO: create visual regressions with puppeteer or cypress'",
    "updtr": "updtr --use yarn --test-stdout"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --bail --verbose"
    }
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rxjs": "^6.5.2",
    "rxjs-hooks": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-react-jsx-self": "7.2.0",
    "@babel/plugin-transform-react-jsx-source": "7.5.0",
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.17",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.3",
    "babel-preset-react-app": "^9.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "compression-webpack-plugin": "3.0.0",
    "concurrently": "4.1.2",
    "css-loader": "3.2.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "file-loader": "4.2.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "gh-pages": "2.1.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^3.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-dom": "4.0.0",
    "jest-resolve": "24.9.0",
    "mini-css-extract-plugin": "0.8.0",
    "npm-check-updates": "3.1.21",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-app-polyfill": "1.0.2",
    "react-dev-utils": "9.0.3",
    "resolve": "1.12.0",
    "rimraf": "3.0.0",
    "sass-loader": "7.2.0",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "terser-webpack-plugin": "1.4.1",
    "ts-shader-loader": "1.0.5",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "updtr": "^3.1.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ]
